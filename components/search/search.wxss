/* components/search/search.wxss */
.search {
	width: 90%;
	color: #333;
	font-size: 24rpx;
	/* position: absolute; */
	border-radius: 50px;
	background-color: #fff;
	padding-left: 14px;
	height: 9vw;
	transition: all 1s ease-in-out;
}
.input-container {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 10vw;
	/* margin-top: 24px; */
	/* background-color: #fff; */
	position: relative;
	background-color: #fdcd3f;
}
.search-image {
	position: absolute;
	right: 9%;
	bottom: 10%;

}
.search-image image{
	width: 35rpx;
	height: 35rpx;
  }